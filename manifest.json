{
    "name": "Take-A-Break",
    "version": "0.0.1",
    "manifest_version": 3,
    "background":{
        "service_worker": "background.js"
    },
    "browser_action": { 
        "default_popup": "popup.html",
        "default_icon": "logo.png"
    },
    "icons": {
            "128": "logo.png"
    },
    "permissions": ["activeTab","notification","storage","alarms"]
}